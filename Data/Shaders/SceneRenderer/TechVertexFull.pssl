struct VS_INPUT
{
	float3 Position		: POSITION0;
	float2 UV			: TEXCOORD0;
	float3 Normal		: TEXCOORD1;
	float3 Tangent		: TEXCOORD2;
};


struct VS_OUTPUT
{
	float4 WorldPosition  : S_POSITION;
};

// To implement
float3x3 inverse(float3x3 m)
{
	return m;
}



VS_OUTPUT main(VS_INPUT input)
{
	VS_OUTPUT output;
	
	output.WorldPosition.xyz = input.Position;

	
	return output;
}